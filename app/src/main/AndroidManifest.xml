<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edit.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.edit.musicalstructure.PopActivityAlbum"
            android:label="@string/category_pop" />
        <activity
            android:name="com.example.edit.musicalstructure.FilmActivityAlbum"
            android:label="@string/category_film" />
        <activity
            android:name="com.example.edit.musicalstructure.RockActivityAlbum"
            android:label="@string/category_rock" />
        <activity
            android:name="com.example.edit.musicalstructure.HipHopActivityAlbum"
            android:label="@string/category_hiphop" />
    </application>

</manifest>